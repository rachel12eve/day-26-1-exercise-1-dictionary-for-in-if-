import random
name=["Alex", "Beth", "Chris","Derek", "Emma","Fiona"]
student_score={student:random.randint(1,100) for student in name}
print(student_score)
#new_dict={new_key:new_value for (key,value) in dictory.items()}
pass_student={student:score for (student,score) in student_score.items() if score>50}

print(pass_student)
